#include <iostream>
using namespace std;
 
int main(){
    int n1,n2,n3,n4;
    cin >> n1 >> n2 >> n3 >> n4;
    string s; 
    cin >> s;
 
    int counter=0;
    for (int i=0;i<s.length();i++){
            if (s[i]=='1'){
                    counter+=n1;
            } if (s[i]=='2'){
                    counter += n2;
            } if (s[i]=='3'){
                    counter += n3;
            } if (s[i]=='4'){
                    counter += n4;
            }
    }
    cout << counter;
return 0;
}
